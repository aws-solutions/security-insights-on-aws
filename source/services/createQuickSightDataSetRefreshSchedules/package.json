{
  "name": "security-lake-configuration",
  "version": "1.0.0",
  "description": "Lambda function to get security lake configuration",
  "main": "./index.js",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com/solutions"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-quicksight": "^3.540.0",
    "winston": "~3.12.0",
    "@types/aws-lambda": "~8.10.136",
    "axios": "^1.6.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^16.18.91",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "aws-sdk-client-mock": "^3.1.0",
    "aws-sdk-client-mock-jest": "^3.1.0"
  },
  "scripts": {
    "pretest": "npm ci",
    "test": "AWS_REGION=us-east-1 npx jest",
    "build:clean": "rm -rf ./node_modules && rm -rf ./dist",
    "build:ts": "npx tsc --project ./tsconfig.json",
    "postinstall": "npm run build:ts",
    "build:copy": "cp -r ./node_modules ./dist/node_modules",
    "build:zip": "cd ./dist && zip -rq createQuickSightDataSets.zip .",
    "build:all": "npm run build:clean && npm ci && npm prune --production && npm run build:copy",
    "coverage": "nyc npm test"
  }
}
